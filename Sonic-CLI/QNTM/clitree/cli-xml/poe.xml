<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY START_PORT_ID "1">
<!ENTITY MAX_PORT_ID "32">
<!ENTITY START_SUB_PORT_ID "1">
<!ENTITY MAX_SUB_PORT_ID "4">
<!ENTITY MAX_MTU "9216">
]>
<CLISH_MODULE xmlns="http://www.dellemc.com/sonic/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xsi:schemaLocation="http://www.dellemc.com/sonic/XMLSchema http://www.dellemc.com/sonic/XMLSchema/clish.xsd">
  <!--=========================================================================-->
  <PTYPE name="POE_POWER_USG_THRESHOLD_TYPE" method="integer" pattern="1..99" help="PoE power usage threshold"/>
  <PTYPE name="POE_POWER_LIMIT_USER_DEF" method="integer" pattern="0..99900" help="PoE port power limit value"/>
  <VIEW name="configure-view">
    <COMMAND name="poe" access="netadmin" help="PoE configuration"/>
    <COMMAND name="no poe" access="netadmin" help="PoE configuration"/>
    <COMMAND name="poe power" access="netadmin" help="PoE power management model configuration"/>
    <COMMAND name="no poe power" access="netadmin" help="PoE power management model configuration"/>
    <COMMAND name="poe power management" access="netadmin" help="Configure PoE power management model" command_tables="sonic-poe:sonic-poe/POE/POE_LIST/id={id}" command_keys="id=GLOBAL" render_command_cb="show_runn_poe_global_power_mgmt_model">
      <PARAM name="power-mgmt-model" help="Power Management Model" ptype="SUBCOMMAND" mode="switch" dbpath="sonic-poe:sonic-poe/POE/POE_LIST/power_management_model">
        <PARAM name="DYNAMIC" help="Dynamic Power Management" ptype="SUBCOMMAND" mode="subcommand" value="dynamic"/>
        <PARAM name="DYNAMIC_PRI" help="Dynamic Priority Power Management" ptype="SUBCOMMAND" mode="subcommand" value="dynamic-priority"/>
        <PARAM name="STATIC" help="Static Power Management" ptype="SUBCOMMAND" mode="subcommand" value="static"/>
        <PARAM name="STATIC_PRI" help="Static Priority Power Management" ptype="SUBCOMMAND" mode="subcommand" value="static-priority"/>
        <PARAM name="CLASS" help="Class based Power Management" ptype="SUBCOMMAND" mode="subcommand" value="class"/>
      </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_poe update_poe_power_mgmt_model GLOBAL ${power-mgmt-model}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE Power management model</DESCRIPTION>
        <USAGE>Use this command to set PoE Power management model</USAGE>
        <EXAMPLE summary="Configure PoE Power management model">
                     sonic-cli(config)# poe power management dynamic
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe power management" access="netadmin" help="Reset PoE power management model">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_poe_power_mgmt_model GLOBAL</ACTION>
      <DOCGEN>
        <DESCRIPTION>Reset PoE Power management model</DESCRIPTION>
        <USAGE>Use this command to reset PoE Power management model</USAGE>
        <EXAMPLE summary="Reset PoE Power management model">
                     sonic-cli(config)# no poe power management
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe usage-threshold" access="netadmin" help="Configure PoE power usage threshold" command_tables="sonic-poe:sonic-poe/POE/POE_LIST/id">
      <PARAM name="usg-threshold" help="Power Usage Threshold" ptype="POE_POWER_USG_THRESHOLD_TYPE" dbpath="sonic-poe:sonic-poe/POE/POE_LIST/power_usage_threshold"/>
      <ACTION builtin="clish_pyobj">sonic_cli_poe update_poe_usage_threshold GLOBAL ${usg-threshold}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE power usage threshold value</DESCRIPTION>
        <USAGE>Use this command to set PoE power usage threshold value</USAGE>
        <EXAMPLE summary="Configure PoE power usage threshold value">
                     sonic-cli(config)# poe usagethreshold 99
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe usage-threshold" access="netadmin" help="Reset PoE power usage threshold">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_poe_usage_threshold GLOBAL</ACTION>
      <DOCGEN>
        <DESCRIPTION>Reset PoE power usage threshold</DESCRIPTION>
        <USAGE>Use this command to reset PoE power usage threshold value</USAGE>
        <EXAMPLE summary="Rest PoE power usage threshold value">
                     sonic-cli(config)# no poe usagethreshold
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe auto-reset" access="netadmin" help="Configure PoE global auto reset mode" command_tables="sonic-poe:sonic-poe/POE/POE_LIST/id" dbpath="sonic-poe:sonic-poe/POE/POE_LIST/auto_reset_mode">
      <ACTION builtin="clish_pyobj">sonic_cli_poe update_poe_auto_reset GLOBAL true</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE global auto reset mode</DESCRIPTION>
        <USAGE>Use this command to set PoE global auto reset mode</USAGE>
        <EXAMPLE summary="Configure PoE global auto reset mode">
                     sonic-cli(config)# poe auto-reset
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe auto-reset" access="netadmin" help="Disable PoE global auto reset mode">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_poe_auto_reset GLOBAL</ACTION>
      <DOCGEN>
        <DESCRIPTION>Disable PoE global auto reset mode</DESCRIPTION>
        <USAGE>Use this command to disable PoE global auto reset mode</USAGE>
        <EXAMPLE summary="Disable PoE global auto reset mode">
                     sonic-cli(config)# no poe auto-reset
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe card" access="netadmin" help="Configure PoE card attributes" view="configure-poe-card-view" viewid="id=${id}" view_keys="id=*" view_tables="sonic-poe:sonic-poe/POE_CARD/POE_CARD_LIST/id={id}">
      <PARAM name="id" help="Card identifier number" ptype="RANGE_0_7" dbpath="sonic-poe:sonic-poe/POE_CARD/POE_CARD_LIST/id"/>
      <ACTION builtin="clish_pyobj">sonic_cli_poe create_poe_card_entry ${id}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE card attributes</DESCRIPTION>
        <USAGE>Use this command to set PoE card attributes</USAGE>
        <EXAMPLE summary="Configure PoE card attributes">
                     sonic-cli(config)# poe card 1
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe card" access="netadmin" help="Delete PoE card configuration">
      <PARAM name="id" help="Card identifier number" ptype="RANGE_0_7"/>
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_poe_card_entry ${id}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Deletes PoE card configuration</DESCRIPTION>
        <USAGE>Use this command to delete PoE card configuration</USAGE>
        <EXAMPLE summary="Delete PoE card configuration">
                     sonic-cli(config)# no poe card 1
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
  </VIEW>
  <!-- PoE Card Level CLIs -->
  <VIEW name="configure-poe-card-view" prompt="${SYSTEM_NAME}(${__config_mode}-poe-card-${id})# " depth="2">
    <NAMESPACE ref="hidden-view" help="false" completion="false"/>
    <NAMESPACE ref="enable-view" help="false" completion="false"/>
    <NAMESPACE ref="enable-view" help="true" prefix="do" completion="true"/>
    <COMMAND name="exit" help="Exit from current mode" lock="false">
            	  <ACTION builtin="clish_nested_up"/>
  </COMMAND>
    <COMMAND name="end" help="Exit to EXEC mode" view="enable-view">
                 <ACTION builtin="clish_close"/>
             </COMMAND>
    <NAMESPACE ref="configure-view" help="false" completion="false"/>
    <COMMAND name="show configuration" access="netadmin" help="show configuration for PoE card">
      <ACTION builtin="clish_pyobj">sonic_cli_show_config show_view views=configure-poe-card view_keys="id=${id}" </ACTION>
      <xi:include href="include/pipe.xml"/>
    </COMMAND>
    <COMMAND name="poe" access="netadmin" help="PoE card configuration"/>
    <COMMAND name="no poe" access="netadmin" help="PoE card configuration"/>
    <COMMAND name="poe power" access="netadmin" help="PoE power management model configuration"/>
    <COMMAND name="no poe power" access="netadmin" help="PoE power management model configuration"/>
    <COMMAND name="poe power management" access="netadmin" help="Configure PoE power management model" command_tables="sonic-poe:sonic-poe/POE_CARD/POE_CARD_LIST/id={id}" command_keys="id=*" render_command_cb="show_runn_poe_card_power_mgmt_model">
      <PARAM name="power-mgmt-model" help="Power Management Model" ptype="SUBCOMMAND" mode="switch" dbpath="sonic-poe:sonic-poe/POE_CARD/POE_CARD_LIST/power_management_model">
        <PARAM name="DYNAMIC" help="Dynamic Power Management" ptype="SUBCOMMAND" mode="subcommand" value="dynamic"/>
        <PARAM name="DYNAMIC_PRI" help="Dynamic Priority Power Management" ptype="SUBCOMMAND" mode="subcommand" value="dynamic-priority"/>
        <PARAM name="STATIC" help="Static Power Management" ptype="SUBCOMMAND" mode="subcommand" value="static"/>
        <PARAM name="STATIC_PRI" help="Static Priority Power Management" ptype="SUBCOMMAND" mode="subcommand" value="static-priority"/>
        <PARAM name="CLASS" help="Class based Power Management" ptype="SUBCOMMAND" mode="subcommand" value="class"/>
      </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_poe update_poe_power_mgmt_model ${id} ${power-mgmt-model}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE Power management model</DESCRIPTION>
        <USAGE>Use this command to set PoE Power management model</USAGE>
        <EXAMPLE summary="Configure PoE Power management model">
                     sonic-cli(config)# poe power management dynamic
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe power management" access="netadmin" help="Reset PoE power management model">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_poe_power_mgmt_model ${id}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Reset PoE Power management model</DESCRIPTION>
        <USAGE>Use this command to reset PoE Power management model</USAGE>
        <EXAMPLE summary="Reset PoE Power management model">
                     sonic-cli(config)# no poe power management
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe usage-threshold" access="netadmin" help="Configure PoE power usage threshold" command_tables="sonic-poe:sonic-poe/POE_CARD/POE_CARD_LIST/id={id}" command_keys="id=*">
      <PARAM name="usg-threshold" help="Power Usage Threshold" ptype="POE_POWER_USG_THRESHOLD_TYPE" dbpath="sonic-poe:sonic-poe/POE_CARD/POE_CARD_LIST/power_usage_threshold"/>
      <ACTION builtin="clish_pyobj">sonic_cli_poe update_poe_usage_threshold ${id} ${usg-threshold}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE power usage threshold value</DESCRIPTION>
        <USAGE>Use this command to set PoE power usage threshold value</USAGE>
        <EXAMPLE summary="Configure PoE power usage threshold value">
                     sonic-cli(config)# poe usagethreshold 99
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe usage-threshold" access="netadmin" help="Reset PoE power usage threshold">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_poe_usage_threshold ${id}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Reset PoE power usage threshold</DESCRIPTION>
        <USAGE>Use this command to reset PoE power usage threshold value</USAGE>
        <EXAMPLE summary="Rest PoE power usage threshold value">
                     sonic-cli(config)# no poe usagethreshold
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe auto-reset" access="netadmin" help="Configure PoE auto reset mode" command_tables="sonic-poe:sonic-poe/POE_CARD/POE_CARD_LIST/id={id}" command_keys="id=*" dbpath="sonic-poe:sonic-poe/POE_CARD/POE_CARD_LIST/auto_reset_mode=true">
      <ACTION builtin="clish_pyobj">sonic_cli_poe update_poe_auto_reset ${id} true</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE auto reset mode</DESCRIPTION>
        <USAGE>Use this command to set PoE auto reset mode</USAGE>
        <EXAMPLE summary="Configure PoE auto reset mode">
                     sonic-cli(config)# poe auto-reset
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe auto-reset" access="netadmin" help="Disable PoE auto reset mode">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_poe_auto_reset ${id}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Disable PoE auto reset mode</DESCRIPTION>
        <USAGE>Use this command to disable PoE auto reset mode</USAGE>
        <EXAMPLE summary="Disable PoE auto reset mode">
                     sonic-cli(config)# no poe auto-reset
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
  </VIEW>
  <!-- PoE Port Level CLIs -->
  <VIEW name="configure-if-view">
    <COMMAND name="poe" help="PoE configuration" access="netadmin" command_tables="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/ifname={name}" command_keys="name=*"/>
    <COMMAND name="no poe" access="netadmin" help="Remove port level all PoE configuration">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_sonic_poe_sonic_poe_poe_port_poe_port_list ${iface}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Removes all PoE configurations at port level</DESCRIPTION>
        <USAGE>Use this command to remove all PoE configuration at port level</USAGE>
        <EXAMPLE summary="Remove all PoE configurations at port level">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# no poe
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe disable" access="netadmin" help="Disable PoE at port level" command_tables="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/ifname={name}" dbpath="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/admin_mode=false">
      <ACTION builtin="clish_pyobj">sonic_cli_poe patch_sonic_poe_sonic_poe_poe_port_poe_port_list_admin_mode ${iface} false</ACTION>
      <DOCGEN>
        <DESCRIPTION>Disable PoE at port level</DESCRIPTION>
        <USAGE>Use this command to Disable PoE at port level</USAGE>
        <EXAMPLE summary="Disable PoE at port level">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# poe disable
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe disable" access="netadmin" help="Enable PoE at port level">
      <ACTION builtin="clish_pyobj">sonic_cli_poe patch_sonic_poe_sonic_poe_poe_port_poe_port_list_admin_mode ${iface} true</ACTION>
      <DOCGEN>
        <DESCRIPTION>Enable PoE at port level</DESCRIPTION>
        <USAGE>Use this command to enable PoE at port level</USAGE>
        <EXAMPLE summary="Enable PoE at port level">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# no poe disable
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe power-limit" access="netadmin" help="Configure PoE power limit" command_tables="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/ifname={name}" command_keys="name=*" render_command_cb="show_runn_poe_port_power_limit">
      <PARAM name="port-power-limit" help="" ptype="SUBCOMMAND" mode="switch" dbpath="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/power_limit">
        <PARAM name="dot3af" help="Class based port power limit" ptype="SUBCOMMAND" mode="subcommand" value="class-based"/>
        <PARAM name="USER" help="User-defined port power limit" ptype="SUBCOMMAND" mode="subcommand" value="user-defined">
          <PARAM name="user-def" help="User-defined port power limit" ptype="POE_POWER_LIMIT_USER_DEF"/>
        </PARAM>
      </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_poe patch_sonic_poe_sonic_poe_poe_port_poe_port_list_power_limit ${iface} ${__params}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE Power limit for a port</DESCRIPTION>
        <USAGE>Use this command to set PoE Power limit for a port</USAGE>
        <EXAMPLE summary="Configure PoE Power limit for a port">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# poe power-limit dot3af
                     sonic-cli(config)# interface Ethernet 4
                     sonic-cli(conf-if-Ethernet4)# poe power-limit 4000
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe power-limit" access="netadmin" help="Reset PoE power limit on port">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_sonic_poe_sonic_poe_poe_port_poe_port_list_power_limit ${iface}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Reset PoE Power limit on port</DESCRIPTION>
        <USAGE>Use this command to reset PoE Power limit on port</USAGE>
        <EXAMPLE summary="Reset PoE Power limit on port">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# no poe power-limit
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe priority" access="netadmin" help="Configure PoE port priority" command_tables="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/ifname={name}" command_keys="name=*" render_command_cb="show_runn_poe_port_priority">
      <PARAM name="port-priority" help="Port Priority" ptype="SUBCOMMAND" mode="switch" dbpath="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/priority">
        <PARAM name="low" help="Low Priority" ptype="SUBCOMMAND" mode="subcommand" value="low"/>
        <PARAM name="medium" help="Medium Priority" ptype="SUBCOMMAND" mode="subcommand" value="medium"/>
        <PARAM name="high" help="High Priority" ptype="SUBCOMMAND" mode="subcommand" value="high"/>
        <PARAM name="critical" help="Critical Priority" ptype="SUBCOMMAND" mode="subcommand" value="critical"/>
      </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_poe patch_sonic_poe_sonic_poe_poe_port_poe_port_list_priority ${iface} ${port-priority}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE port priority</DESCRIPTION>
        <USAGE>Use this command to set PoE port priority</USAGE>
        <EXAMPLE summary="Configure PoE port priority">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# poe priority medium
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe priority" access="netadmin" help="Reset PoE port priority">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_sonic_poe_sonic_poe_poe_port_poe_port_list_priority ${iface}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Reset PoE port priority</DESCRIPTION>
        <USAGE>Use this command to reset PoE port priority</USAGE>
        <EXAMPLE summary="Rest PoE port priority">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# no poe priority
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe detection" access="netadmin" help="Configure PoE detection mode for port" command_tables="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/ifname={name}" command_keys="name=*" render_command_cb="show_runn_poe_port_detection">
      <PARAM name="port-poe-detect" help="PoE detection mode" ptype="SUBCOMMAND" mode="switch" dbpath="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/detection_mode">
        <PARAM name="two_pt_dot3af" help="2 Point Dot3af detection" ptype="SUBCOMMAND" mode="subcommand" value="2pt-dot3af"/>
        <PARAM name="two_pt_dot3af_leg" help="2 Point Dot3af detection followed by Legacy detection" ptype="SUBCOMMAND" mode="subcommand" value="2pt-dot3af+legacy"/>
        <PARAM name="four_pt_dot3af" help="4 Point Dot3af detection" ptype="SUBCOMMAND" mode="subcommand" value="4pt-dot3af"/>
        <PARAM name="four_pt_dot3af_leg" help="4 Point Dot3af detection followed by Legacy detection" ptype="SUBCOMMAND" mode="subcommand" value="4pt-dot3af+legacy"/>
        <PARAM name="dot3bt" help="Dot3bt detection" ptype="SUBCOMMAND" mode="subcommand" value="dot3bt"/>
        <PARAM name="dot3bt_leg" help="Dot3bt detection followed by Legacy detection" ptype="SUBCOMMAND" mode="subcommand" value="dot3bt+legacy"/>
        <PARAM name="legacy" help="Legacy detection" ptype="SUBCOMMAND" mode="subcommand" value="legacy"/>
      </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_poe patch_sonic_poe_sonic_poe_poe_port_poe_port_list_detection_mode ${iface} ${port-poe-detect}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE detection mode for port</DESCRIPTION>
        <USAGE>Use this command to set PoE detection mode for port</USAGE>
        <EXAMPLE summary="Configure PoE detection mode for port">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# poe detection 2ptdot3af
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe detection" access="netadmin" help="Reset PoE detection mode for port">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_sonic_poe_sonic_poe_poe_port_poe_port_list_detection_mode ${iface}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Reset PoE detection mode for port</DESCRIPTION>
        <USAGE>Use this command to reset PoE detection mode for port</USAGE>
        <EXAMPLE summary="Rest PoE detection mode for port">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# no poe detection
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="poe power-up" access="netadmin" help="PoE power-up configuration"/>
    <COMMAND name="no poe power-up" access="netadmin" help="Remove PoE power-up configuration"/>
    <COMMAND name="poe power-up mode" access="netadmin" help="Configure PoE power-up mode" command_tables="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/ifname={name}" command_keys="name=*" render_command_cb="show_runn_poe_port_powerup_mode">
      <PARAM name="port-poe-powerup" help="PoE power-up mode" ptype="SUBCOMMAND" mode="switch" dbpath="sonic-poe:sonic-poe/POE_PORT/POE_PORT_LIST/powerup_mode">
        <PARAM name="IEEE_8023AT" help="Dot3at power-up mode" ptype="SUBCOMMAND" mode="subcommand" value="dot3at"/>
        <PARAM name="PRE_8023AT" help="Pre-Dot3at power-up mode" ptype="SUBCOMMAND" mode="subcommand" value="pre-dot3at"/>
        <PARAM name="PRE_8023BT" help="Pre-Dot3bt power-up mode" ptype="SUBCOMMAND" mode="subcommand" value="pre-dot3bt"/>
        <PARAM name="IEEE_8023BT_TYPE3" help="Dot3bt Type3 power-up mode" ptype="SUBCOMMAND" mode="subcommand" value="dot3bt-type3"/>
        <PARAM name="IEEE_8023BT_TYPE4" help="Dot3bt Type4 power-up mode" ptype="SUBCOMMAND" mode="subcommand" value="dot3bt-type4"/>
        <PARAM name="IEEE_8023BT" help="Dot3bt power-up mode" ptype="SUBCOMMAND" mode="subcommand" value="dot3bt"/>
        <PARAM name="IEEE_8023AF" help="Dot3af power-up mode" ptype="SUBCOMMAND" mode="subcommand" value="dot3af"/>
        <PARAM name="HIGH_INRUSH" help="High Inrush power-up mode" ptype="SUBCOMMAND" mode="subcommand" value="high-inrush"/>
      </PARAM>
      <ACTION builtin="clish_pyobj">sonic_cli_poe patch_sonic_poe_sonic_poe_poe_port_poe_port_list_powerup_mode ${iface} ${port-poe-powerup}</ACTION>
      <DOCGEN>
        <DESCRIPTION>Configure PoE port power-up mode</DESCRIPTION>
        <USAGE>Use this command to set PoE port power-up mode</USAGE>
        <EXAMPLE summary="Configure PoE port power-up mode">
                     sonic-cli(config)# interface Ethernet 0
                     sonic-cli(conf-if-Ethernet0)# poe power-up mode dot3at
                 </EXAMPLE>
      </DOCGEN>
    </COMMAND>
    <COMMAND name="no poe power-up mode" access="netadmin" help="Reset PoE power-up mode for port">
      <ACTION builtin="clish_pyobj">sonic_cli_poe delete_sonic_poe_sonic_poe_poe_port_poe_port_list_powerup_mode ${iface}</ACTION>
      <DOCGEN>
